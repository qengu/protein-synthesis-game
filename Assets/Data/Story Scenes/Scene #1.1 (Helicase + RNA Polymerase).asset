    displayImages: []
    italic: 0
    blackScreenDisplay: 0
  - speaker: {fileID: 11400000, guid: 712ccd152c163b44baf2260e6720f74c, type: 2}
    text: '*My job is so boring. All I do is break hydrogen bonds between the complementary
      bases.*'
    actions: []
    animation: 0
    animationTime: 0
    displayImages: []
    italic: 1
    blackScreenDisplay: 0
  - speaker: {fileID: 11400000, guid: 6a8d2d8daa099324b8c4d860fd227e5e, type: 2}
    text: Finally! What took you so long?
    actions:
    - speaker: {fileID: 11400000, guid: 6a8d2d8daa099324b8c4d860fd227e5e, type: 2}
